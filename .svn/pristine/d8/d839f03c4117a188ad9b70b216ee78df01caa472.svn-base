	text-decoration:none;
	font-size:12px;
	
}
#stationDialog li{
	display:inline-block;
}
#stationDialog .modelUl li{
	min-width: 69px;
}
/* 整个的div内容 */
#stationDialog{
	width:400px;
	position: absolute;
	z-index:4;
	height:300px;
	background:#4F95EA;
	margin:0 auto;
	font-family:'Microsoft YaHei';
	border-radius:5px;
}

/* 数据源的标题 */
#stationDialog header{
	height:35px;
	line-height:35px;
	color:#fff;
	font-size:12px;
	padding-left:10px;
}

/* 模式选择和检验排名 */
#stationDialog div#dialogContent{
	height:253px;
	background:#fff;
	width:386px;
	margin:0 auto;
	/* padding-left:10px;
	padding-top:10px; */
}

/* 模式选择  */
#stationDialog .model{
	width:225px;
	height:155px;
	margin: 5px 0px 0px 6px;
	background:#EEF3F7;
	float:left;
	border-radius:5px;
	padding-top:5px;
	padding-left:5px;
	margin-bottom:10px;
}
/* 模式选择下的列表  */
#stationDialog ul.modelUl{
	width:228px;
	height:125px;
	padding: 0px;
}
#stationDialog ul.modelUl li{
	/* width:65px; */
	height:23px;
}
#stationDialog ul.modelUl li span{
	padding: 0px 3px 0px 3px;
	cursor: pointer;
}
#stationDialog a.common-a{
	width:68px;
	display:inline-block;
	color:#609FF6;
	padding:3px 0 3px 5px;
	cursor:pointer;
}

#stationDialog .modelUl-a{
	background:#4A91E9;
	padding: 0px 6px 0px 6px;
	color:#fff !important;
	border-radius:4px;
}
#stationDialog .modelUl-unclick{
	background:#BDBFC2;
	padding: 0px 6px 0px 6px;
	cursor: context-menu !important;
	color:#fff !important;
	border-radius:4px;
}
/*  检验排名*/
#stationDialog .rank{
	width:141px;
	height:155px;
	background:#EEF3F7;
	margin: 5px 0px 0px 7px;
	float:left;
	border-radius:5px;
	padding-top:5px;
	padding-left:5px;
	margin-bottom:10px;
}
#stationDialog .rank ul{
	margin-top:10px;
	margin-left:5px;
	padding: 0px;
	font-size:14px;
	color:#437BF1;
}

/* 检验排名下的圆形图标 */
