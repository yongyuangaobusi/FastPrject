.wtbcontent {
	position: relative;
	overflow: visible;
	width: 56px;
	height: 100%;
	background-color: rgba(33, 48, 69, 0.69);
	border: 1px solid #2a292f;
	color: #def9fd;
	font-size: 13px;
	font-family: "Microsoft YaHei";
	border-top: none;
}

.wtbtop {
	width: 100%;
	height: 26px;
	background-image: url("icons/weatherTimer-top.png");
	background-position: 50% 50%;
	background-repeat: no-repeat;
}

.wtbtop-icon {
	width: 30px;
	height: 26px;
	line-height: 26px;
	background-position: 8px 9px;
	background-repeat: no-repeat;
	text-align: right;
	padding-right: 5px;
	color: #fff;
	float: left;
	cursor: pointer;
}

.wtbtop-icon-off{
	background-image: url("icons/arrows-right.png");
}

.wtbtop-icon-on{
	background-image: url("icons/arrows-left.png");
}

.wtbtop-icon-close{
	height: 10px;
	width: 10px;
	line-height: 10px;
	text-align: center;
	float: right;
	color: #fff;
	cursor: pointer;
}

.wtbele {
	position: relative;
	height: auto;
	margin-bottom: 1px;
}

.wtbicon-wrap{
	position: relative;
	width: 22px;
	height: 22px;
	line-height: 15px;
	float: right;
	margin-right: 10px;
	background-color: #fff;
	border: 1px solid #D5DCE6;
	border-radius:3px;
	text-align: center;
	overflow: hidden;
	background-repeat: no-repeat;
	background-position: center center;
	background-size: auto;
	padding: 2px;
	cursor: pointer;
	border: 1px solid #000000;
	background-color: #feffcc;
}

.wtbicon-wrap img{
	display: inline-block;
	height: 16px;
    vertical-align: -1px;
}

.wtbweather {
	position: absolute;
	right: 60px;
	height: 22px;
}

.skdata .wtbweather{
	margin-top: 2px;
	right: 50px;
}

.dlweather{
	border: 1px solid #000000;
	background-color: #ffffcb;
	margin-right: 10px;
	right: 50px;
    top: 3px;
}

.dqweather{
	border: 1px solid #000000;
	background-color: #ffffcb;
	margin-right: 10px;
	right: 50px;
}

.wtbicon-wrap-warning{
	border: 1px solid #f93218 !important;
	background-color: #fdf4f2 !important;
}

.wtbskcontent{
	border-bottom: 1px solid #686868;
	cursor: pointer;
}

.wtbskcontent .wtbweather{
	right: 50px;
}

.wtbskcontent .wtbtext{
	margin-top: 5px;
}

.wtbdlcontent .wtbweather{
	right: 50px;
}

.wtbdqcontent .wtbweather{
	right: 50px;
}


.dlweather .wtbicon-wrap:last-child{
	margin-left: 0px;
    margin-right: 0px;
}

.dqweather .wtbicon-wrap:last-child{
    margin-left: 0px;
    margin-right: 0px;
}

.dlweather .wtbicon-wrap, .dqweather .wtbicon-wrap{
	border: none;
	background-color: transparent;
	width: 20px;
	cursor: default;
    margin-left: 5px;
    margin-right: 5px;
    float: left;
}

.wtbdateweather .wtbicon-wrap{
	cursor: default;
}

.wtbicon-wrap .icon-icon-round-1{
	font-weight: bold;
}

.wtbicon-wrap.wtbicon-activitied{
	border: 1px solid #4695DA !important;
}

.wtbicon {
	height: 100%;
	font-size: 20px;
}

img.wtbicon{
	padding: 0 3px;
}
.wtbicon:HOVER{
	background-color: rgba(5, 151, 255, 0.52);
	color: #fff !important;
}
.wtbicon-activitied .wtbicon{
	background-color: rgba(5, 151, 255, 0.52);
	color: #fff !important;
}

.wtbtext {
	width: 100%;
	height: 22px;
	line-height: 22px;
	margin: 0px;
	text-align: center;
    cursor: pointer;
}

.skdata{
	border-bottom: 2px solid rgba(169, 195, 216, 0.5);
}

.wtbdlcontent{
	
}

.wtbtitle {
	position: relative;
}

.dqtitle{
    border-top: 2px solid rgba(169, 195, 216, 0.5);
}

.wtbtitle .wtbfonttitle-top-line{
    width: 43px;
    margin-left: 5px;
    border-top: 1px dotted rgba(169, 195, 216, 0.8);
}

.wtbtitle .wtbfonttitle {
	text-align: center;
	cursor: pointer;
	height: 28px;
	width: 100%;
	line-height: 28px;
}

.wtbdqcontent{
}

.skdata{
}

.wtbtextactive {
	background-color: rgba(33, 48, 69, 0.5);
}

.wtbfonttactive {
	background-color: rgba(33, 48, 69, 0.5);
}

.wtbtext-selected{
	background-color: rgba(53, 128, 240, 1);
}

.wtbdlcontent .wtbtext:HOVER{
	background-color: rgba(53, 128, 240, 1);
}

.wtbdatecontent .wtbtext:HOVER{
	background-color: rgba(53, 128, 240, 1);
}

.wtbdate {
	
}

.wtbdqcontent .wtbele{
	cursor: default;
}

.icon-icon-gaowen-2 .path1:before {
	color: rgb(246, 109, 73);
}

.wtbcontent .kg-feedback-all-know{
	height: 18px;
	width: 100%;
	margin: 2px 0;
	background: url("icons/kg-feedback-all-know.png");
	background-repeat: no-repeat;
	background-position: center center;
	background-size: contain;
	cursor: pointer;
}

.weather-bar-expand-wrap{
	position: absolute;
	right: 60px;
	top: 55px;
	background-color: rgba(33, 48, 69, 0.69);
	border: 1px solid #2a292f;
	border-radius: 5px;
	z-index: 9998;
	max-width: 1502px;
	overflow-y: hidden;
	overflow-x: auto;
}

.weather-bar-expand-wrap .weather-bar-expand{
	height: 100%;
}

.weather-bar-expand-wrap .weather-bar-expand-area-weather-wrap{
	width: 100px;
	float: left;
}

.weather-bar-expand-area-weather-wrap .area-weather-header{
	height: 20px;
	line-height: 20px;
	border-bottom: 1px solid #2a292f;
	color: #def9fd;
	font-weight: bold;
	font-size: 11px;
	text-align: center;
    cursor: default;
}

.weather-bar-expand-area-weather-wrap .area-weather-cell{
	height: 23px;
	line-height: 23px;
	border: 1px solid #2a292f;
	border-top: none;
	border-left: none;
	overflow: hidden;
	text-align: left;
}

.weather-bar-expand-area-weather-wrap .area-weather-header-cell{
	height: 29px;
	line-height: 29px;
}

.weather-bar-expand-area-weather-wrap .area-realtime-weather-wrap{
	height: 30px;
	line-height: 30px;
}

.weather-bar-expand-wrap .weather-bar-expand-area-weather-wrap:last-child .area-weather-cell{
	border-right: none;
}

.weather-bar-expand-wrap .area-weather-icon{
	height: 18px;
	width: 20px;
	margin: 2px;
	margin-bottom: 0px;
	background-repeat: no-repeat;
	background-position: center center;
	background-size: auto;
	line-height: 14px;
	text-align: center;
	border: 1px solid #000000;
	background-color: #feffcc;
	float: left;
}

.area-weather-icon img{
	height: 14px;
}

.weather-bar-expand-wrap .area-weather-icon:FIRST-CHILD {
	margin-left: 3px;
}

.area-realtime-weather-wrap .area-weather-icon{
	margin-top: 6px;
}

.area-weather-header-cell .area-weather-icon{
	margin-top: 5px;
}

.weather-bar-expand-wrap .area-weather-wrap-xf{
	position: absolute;
	border: 1px solid #d6d6d6;
	z-index: 33;
	height: 30px;
	background-color: #fff;
	border-radius: 3px;
	padding-top: 1px;
	padding-left: 2px;
}

.area-weather-cell .area-weather-icon-titan{
	width: 30px !important;
}

.area-weather-cell .area-weather-icon-hail{
	width: 14px !important;
}

